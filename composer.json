{
    "name": "vizra-ai/ai-tokens",
  "version": "0.0.1",
    "description": "Estimate AI API costs before making expensive calls",
    "type": "library",
    "keywords": ["ai", "tokens", "cost", "estimation", "openai", "claude", "laravel", "api", "pricing", "llm", "gpt", "anthropic", "token-counter", "cost-calculator", "api-cost", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Lumsden",
  "version": "0.0.1",
            "email": "aaronlumsden@me.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "illuminate/support": "For Laravel integration"
    },
    "autoload": {
        "psr-4": {
            "VizraAi\\AiTokens\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VizraAi\\AiTokens\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VizraAi\\AiTokens\\Laravel\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}